{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "OpenIdConnect": {
    "Authority": "https://localhost:5001/",
    "ClientId": "manager@olympus.onboardspa.com",
    "DefaultScopes": [
      "openid",
      "profile",
      "olympus.activities",
      "olympus.apigateway",
      "olympus.apiis4",
      "olympus.catalog",
      "olympus.directory",
      "olympus.infrastructure",
